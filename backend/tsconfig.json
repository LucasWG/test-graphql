{
	"compilerOptions": {
		"target": "ES2018",
		"lib": ["ES2018", "ESNext.AsyncIterable"],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"module": "commonjs",
		"outDir": "build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"removeComments": true,
		"strictPropertyInitialization": false
	}
}
